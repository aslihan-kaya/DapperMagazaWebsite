@model IEnumerable<DapperMagazaWebsite.Models.AdminModel>

@{
    ViewBag.Title = "AdminSignUp";
    Layout = "~/Views/Shared/_AdminLayoutPage1.cshtml";
}



<p>
    @Html.ActionLink("Create New", "AdminEY")
</p>
<table class="table">
